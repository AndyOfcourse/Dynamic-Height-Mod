{
  "schema_version": 1,
  "quilt_loader": {
    "group": "net.multiverse.dynamicheight",
    "id": "dynamicheight",
    "version": "${version}",
    "metadata": {
      "name": "Dynamic Height",
      "description": "Change the world height limits - both up and down - without affecting generation or performance!",
      "license": "${mod_license}",
      "contact": {
        "homepage": "${mod_homepage}"
      },
      "contributors": {
        "Multiverse": "Author"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.multiverse.dynamicheight.DynamicHeightMod"
      ],
      "client_init": [
        "net.multiverse.dynamicheight.DynamicHeightClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.26.0"
      },
      {
        "id": "quilt_base",
        "versions": "*"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=11.0.0-alpha.1"
      },
      {
        "id": "minecraft",
        "versions": "*"
      }
    ]
  },
  "mixin": "dynamicheight.mixins.json"
}
